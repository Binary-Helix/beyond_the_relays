entity = {
	name = "mammalian_01_constructor_entity"
	   pdxmesh = "molluscoid_01_corvette_frame_mesh"
   
	   default_state = "idle"
	   state = { name = "idle"	 						animation = "idle" animation_blend_time = 0 }
