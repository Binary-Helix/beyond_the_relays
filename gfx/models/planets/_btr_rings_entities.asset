
# Gas Giant Rings
@rings_scale = 1.3
@rings_height = -115

# Large

entity = {
	name = "gas_giant_rings_01_large_entity"
	cull_radius = 500
	locator = { name = "planet" position = { 0 @rings_height 0 } rotation = { 0 0 3 }}
	attach = { "planet" = "gas_giant_rings_01_large_attach"}
	scale = @rings_scale
}

entity = {
	name = "gas_giant_rings_01_large_attach"
	pdxmesh = "planetary_rings_02_mesh"
	
	meshsettings = {
	name = "patternShape"
	texture_diffuse = "ring_tiling_01_large_diffuse.dds"
	texture_normal = "ring_tiling_01_large_normal.dds"
