### Gas Giants
entity = {
	name = "gas_giant_01_entity"
	pdxmesh = "planet_no_poles_clouded_mesh"

	meshsettings = {
	name = "planet_geosphereShape"
	texture_diffuse = "gas_giant_01_diffuse.dds"
	texture_normal = "gas_giant_01_normal.dds"
	texture_specular = "gas_giant_01_specular.dds"
	}

	##cloud texture override
	meshsettings = {
		name = "clouds_geosphereShape"
		texture_diffuse = "atmosphere_jovian_red.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "atmosphere_specular.dds"
	}
	
	##cloud shadow texture override
	meshsettings = {
		name = "clouds_shadow_geosphereShape"
		texture_diffuse = "atmosphere_medium.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "atmosphere_specular.dds"
	}	

	default_state = "idle"
	#playback_rate = -1.0 # Rotation is reversed
	state = { name = "idle" animation = "idle" time_offset = { 0 100 }
		start_event = { trigger_once = yes sound = { soundeffect = "gas_giant_amb_hum" } }
	}
	scale = 1.2
}

